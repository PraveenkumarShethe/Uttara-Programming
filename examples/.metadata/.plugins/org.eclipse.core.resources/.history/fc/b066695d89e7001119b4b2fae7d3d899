CREATE TABLE COUNTRY (
  cn_id INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  cn_code CHAR(2) NULL,
  cn_name VARCHAR(20) NULL,
  PRIMARY KEY(cn_id)
);

CREATE TABLE employee (
  emp_id INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  cn_id INTEGER UNSIGNED NOT NULL,
  emp_surname VARCHAR(40) NULL,
  emp_firstname VARCHAR(40) NULL,
  PRIMARY KEY(emp_id),
  FOREIGN KEY(cn_id)
    REFERENCES COUNTRY(cn_id)
      ON DELETE NO ACTION
      ON UPDATE NO ACTION
);


